  "hero": {
    "title": "THE SOCIOSCOPE",
    "subtitle": "Seeing How Societies Transform",
    "tagline": "A groundbreaking initiative to understand and map how communities create sustainable food futures"
  },
  "microscope": {
    "icon": "ðŸ”¬",
    "title": "The World's First Social Microscope",
    "description1": "Just as the microscope revolutionized biology by revealing the hidden world of cells, The Socioscope is revealing the hidden patterns of social transformation.",
    "description2": "We're building a systematic way to see what was previously invisible: how individual actions connect to form collective change."
  },
  "vision": {
    "title": "Why This Matters",
    "statement": "We're facing existential challenges: climate collapse, inequality, democratic fragility. We need to understand how societies actually transformâ€”not in theory, but in practice.",
    "details1": "The Socioscope doesn't just produce academic papers. It reveals what makes change work: which interventions succeed, which fail, and why. It shows policymakers what policies actually take root. It helps organizers understand what makes movements scale. It gives communities the knowledge to design their own pathways to transformation.",
    "details2": "This is about building a sustainable future that's not just environmentally sound, but socially resilient and economically just: while contributing to innovative methodologies for data collection, LLM-driven research, and the food system itself."
  },
  "contributions": {
    "card1": {
      "icon": "ðŸ“Š",
      "title": "Data",
      "description": "Sophisticated in-person data collection across 29 countries, capturing not just what initiatives do, but how they transform economic, ecological, cultural, and governance systems.",
      "description2": "Our researchers conduct ethnographic fieldwork, structured interviews, and participatory observation to build the most comprehensive dataset of food system transformations in existence.",
      "link": ""
    },
    "card2": {
      "icon": "ðŸ¤–",
      "title": "Technology",
      "description": "Establishing an LLM-driven research methodology that can process qualitative data at scale while preserving contextual nuance and cultural specificity.",
      "description2": "Built on Sleipnir, our analytical framework enables pattern recognition across hundreds of casesâ€”finding what traditional methods miss.",
      "link": "Explore Sleipnir â†’"
    },
    "card3": {
      "icon": "ðŸŒ¾",
      "title": "Analysis",
      "description": "Developing new frameworks for understanding systems transformation, particularly in food systems where economic, ecological, cultural, and political dimensions intersect.",
      "description2": "Our transaction grid methodology maps how change moves across these dimensionsâ€”revealing the mechanics of societal transformation.",
      "link": ""
    }
  },
  "breakthroughs": {
    "title": "Scientific Breakthroughs",
    "subtitle": "We're not just collecting stories. We're discovering the fundamental laws of how societies transform themselves.",
    "card1": {
      "title": "Universal Patterns Across Cultures",
      "description": "Despite vast cultural differences, we're finding common mechanisms in how communities achieve sustainable changeâ€”from rural Kenya to urban Tokyo."
    },
    "card2": {
      "title": "Cross-Cultural Pattern Recognition",
      "description": "We can now compare how sustainability works in a village facing hunger versus a city drowning in waste versus a community rebuilding after crisisâ€”and discover universal mechanisms beneath surface differences."
    },
    "card3": {
      "title": "From Micro to Macro",
      "description": "For the first time, we can rigorously trace the path from individual choices to community practices to systemic transformation. Social science's equivalent of physics' scaling laws."
    },
    "card4": {
      "title": "A Generalizable Method",
      "description": "We start with food as our \"model organism,\" but this methodology can be applied to any domain of social changeâ€”from climate action to healthcare to democracy itself."
    }
  },
